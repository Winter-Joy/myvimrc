{
    "languageserver": {
        "intelephense": {
            "command": "intelephense",
            "args": ["--stdio"],
            "filetypes": ["php"],
            "initializationOptions": {
                "storagePath": "/tmp/intelephense"
            }
        },
        "python.jediEnabled":false
    },
	"tabnine.shortcut": "TN",
	"tabnine.priority": 100,
	"clangd.enabled": true,
	"clangd.path":"/usr/bin/clangd"
}
